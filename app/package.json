{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "browser": {
    "child_process": false
  },
  "scripts": {
    "build:next": "NODE_ENV=production DATABASE_URL= next build",
    "build:server": "NODE_ENV=production tsc --project tsconfig.server.json",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production APP_ROOT=$(pwd)/.. TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/app/server/cli.js",
    "dev": "NODE_ENV=development ts-node server/cli.ts"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "d3": "^7.8.2",
    "daisyui": "^2.50.0",
    "dotenv": "^16.0.3",
    "events": "^3.3.0",
    "jwt-decode": "^3.1.2",
    "multiparty": "^4.2.3",
    "next": "^14.1.0",
    "next-auth": "^4.19.2",
    "nodemailer": "^6.8.0",
    "prismjs": "^1.29.0",
    "raw-body": "^2.5.2",
    "react-markdown": "^8.0.6",
    "react-masonry-css": "^1.0.16",
    "react-simple-code-editor": "^0.13.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
    "use-async-effect": "^2.2.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/multiparty": "^0.0.33",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
}
