{
  "name": "playbook-partnership",
  "version": "0.45.0",
  "description": "A repository for the playbook partnership collaboration.",
  "private": true,
  "workspaces": [
    "./app",
    "./components/**"
  ],
  "scripts": {
    "build": "npx --workspace=app next build",
    "codegen:apis": "ts-node cli/sync-api-import.ts",
    "codegen:components": "ts-node cli/sync-component-import.ts",
    "codegen:cite": "ts-node cli/add-citation.ts",
    "codegen:icons": "python cli/convert-icons.py icons/services/src icons/services/index.ts",
    "codegen:requirements": "ts-node cli/enumerate-requirements.ts",
    "codegen:sql": "mkdir -p db/migrations && ts-node cli/build-sql.ts > db/migrations/00_init.sql",
    "codegen:table": "ts-node cli/build-table.ts > metanodes.tsv",
    "codegen:analytics": "ts-node cli/dump-analytics.ts > analytics.tsv",
    "codegen:dsl": "ts-node cli/build-dsl.ts > dsl.d.ts",
    "codegen:embeddings": "ts-node -r esm -r tsconfig-paths/register cli/build-embeddings.ts > embeddings.tsv",
    "dev": "npx --workspace=app next dev",
    "dev:ssl": "concurrently \"npm run dev -- -p 3001\" \"ssl-proxy --source 3000 --target 3001\"",
    "start:worker": "ts-node cli/resolve-process-worker.ts",
    "start:run-wes-worker": "ts-node cli/run-wes-worker.ts",
    "start:wes-worker": "NODE_ENV=production ts-node cli/wes-worker.ts",
    "start:ssl": "concurrently \"npm run start -- -p 3001\" \"ssl-proxy --source 3000 --target 3001\"",
    "start": "npx --workspace=app next start",
    "version": "docker build -t maayanlab/playbook-partnership:${npm_package_version} .",
    "postversion": "docker push maayanlab/playbook-partnership:${npm_package_version}"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.5.1",
    "@blueprintjs/icons": "^5.2.1",
    "@blueprintjs/select": "^5.0.16",
    "@blueprintjs/table": "^5.0.16",
    "@mdi/js": "^7.3.67",
    "@types/pluralize": "^0.0.32",
    "classnames": "^2.3.2",
    "fast-sha256": "^1.3.0",
    "form-data": "^4.0.0",
    "glob": "^8.1.0",
    "json-canonicalize": "^1.0.6",
    "pg": "^8.11.3",
    "pg-boss": "^9.0.3",
    "openai": "^3.2.1",
    "pg-listen": "^1.7.0",
    "pg-query-stream": "^4.5.3",
    "pluralize": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "swr": "^2.2.4",
    "ts-dedent": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.8.7",
    "@types/node-fetch": "^2.6.7",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.29",
    "concurrently": "^8.2.2",
    "next": "^13.5.6",
    "next-transpile-modules": "^10.0.1",
    "ssl-proxy": "^1.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "zod-to-ts": "^1.1.4"
  }
}
